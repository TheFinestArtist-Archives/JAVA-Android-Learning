<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="@dimen/list_cell_basic_height">
        <TextView android:id="@id/supertitle_tv" android:visibility="gone" android:layout_marginLeft="@dimen/margin" android:text="@string/create_board_all" style="@style/list_cell_super_text" />
        <RelativeLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <com.pinterest.ui.imageview.GrayWebImageView android:id="@id/icon_wiv" android:visibility="gone" android:layout_width="@dimen/thumbnail_size" android:layout_height="@dimen/thumbnail_size" android:layout_marginLeft="@dimen/margin" android:scaleType="centerCrop" android:layout_centerVertical="true" />
            <TextView android:id="@id/title_tv" android:layout_marginLeft="@dimen/margin" android:layout_marginRight="@dimen/margin" android:layout_toRightOf="@id/icon_wiv" android:layout_centerVertical="true" style="@style/list_cell_text" />
        </RelativeLayout>
    </LinearLayout>
    <View android:id="@id/list_divider" android:visibility="gone" style="@style/list_cell_divider" />
</merge>